// Enlace al simulador: https://www.tinkercad.com/things/8XejGirJsdG-video-2
//https://github.com/santiago12R/Computacion-ubicua/blob/main/Fotos-laboratorios/IMG-20250928-WA0024.jpg
void setup() { 
  pinMode(13, OUTPUT); // pin 13 como salida
} 

void loop() { 
  digitalWrite(13, HIGH); // pin 13 a nivel alto (1) 
  delay(3000);            // demora de 3 seg
  digitalWrite(13, LOW);  // pin 13 a nivel bajo (0) 
  delay(1500);            // demora de 8 seg

}
